{
  "name": "@hawkeye/core",
  "version": "0.2.0",
  "description": "Hawkeye Core Engine - Perception, Reasoning, Execution",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./perception": {
      "types": "./dist/perception/index.d.ts",
      "import": "./dist/perception/index.mjs",
      "require": "./dist/perception/index.js"
    },
    "./reasoning": {
      "types": "./dist/reasoning/index.d.ts",
      "import": "./dist/reasoning/index.mjs",
      "require": "./dist/reasoning/index.js"
    },
    "./execution": {
      "types": "./dist/execution/index.d.ts",
      "import": "./dist/execution/index.mjs",
      "require": "./dist/execution/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint src/ || true",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@types/ws": "^8.5.10",
    "agent-browser": "^0.7.0",
    "chokidar": "^3.5.3",
    "screenshot-desktop": "^1.15.0",
    "temp": "^0.9.4",
    "uuid": "9.0.1",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "@paddlejs-models/ocr": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  }
}
